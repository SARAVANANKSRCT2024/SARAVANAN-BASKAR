#include <stdio.h>

int main()
{
    int num1, num2, smallest;

    // Input two numbers from the user
    printf("Enter the first number: ");
    scanf("%d", &num1);
    printf("Enter the second number: ");
    scanf("%d", &num2);

    // Determine the smallest number
    if (num1 < num2) 
    {
        smallest = num1;
    } 
    else 
    {
        smallest = num2;
    }

    // Display the result
    printf("The smallest number is: %d\n", smallest);

    return 0;
}

