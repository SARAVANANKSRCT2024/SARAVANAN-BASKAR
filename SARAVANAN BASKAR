#include<stdio.h>
void main()
{
    int age;
    printf("enter the age");
    scanf("%d",&age);
    if(age>=1 && age<=12)
    {
        printf("person is child");
    }
    else if(age>=13 && age<=19)
    {
        printf("person is teenager");
    }
    else if(age>20)
    {
        printf("person is adult");
    
    }
}    
